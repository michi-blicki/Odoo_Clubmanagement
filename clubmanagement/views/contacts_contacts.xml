<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data noupdate="0">
    
        <record id="contacts.action_contacts" model="ir.actions.act_window">
            <field name="domain" eval="[('is_club_member', '=', False)]"/>
        </record>

        <record id="contacts.res_partner_menu_config" model="ir.ui.menu">
            <field name="sequence" eval="90" />
        </record>

        <record id="clubmanagement.action_club_contacts" model="ir.actions.act_window">
            <field name="name">Club Contacts</field>
            <field name="res_model">res.partner</field>
            <field name="view_mode">kanban,list,form</field>
            <field name="domain" eval="[('is_club_member', '=', True)]" />
        </record>

        <menuitem id="clubmanagement.menu_contacts_club"
            name="Club Contacts"
            parent="contacts.menu_contacts"
            action="clubmanagement.action_club_contacts"
            groups="clubmanagement.group_clubmanagement_master_user"
            sequence="10"/>
    
    </data>
    

</odoo>
