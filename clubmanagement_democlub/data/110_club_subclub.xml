<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Subclub for manchester_nebula_fc_main_club -->
        <record id="subclub_main_club" model="club.subclub">
            <field name="name">Manchester Nebula FC, Main Club</field>
            <field name="company_id" ref="manchester_nebula_fc_main_club"/>
            <field name="club_id" ref="manchester_nebula_fc_club"/>
            <field name="hr_department_id" ref="dept_executive_board_main_club"/>
            <field name="sequence">10</field>
            <field name="active" eval="True"/>
        </record>

        <!-- Subclub for manchester_nebula_fc_junior -->
        <record id="subclub_junior" model="club.subclub">
            <field name="name">Manchester Nebula FC, Junior Division</field>
            <field name="company_id" ref="manchester_nebula_fc_junior"/>
            <field name="club_id" ref="manchester_nebula_fc_club"/>
            <field name="hr_department_id" ref="dept_executive_board_junior"/>
            <field name="sequence">20</field>
            <field name="active" eval="True"/>
        </record>

        <!-- Subclub for nebula_fc_lucerne -->
        <record id="subclub_lucerne" model="club.subclub">
            <field name="name">Nebula FC, Lucerne Chapter</field>
            <field name="company_id" ref="nebula_fc_lucerne"/>
            <field name="club_id" ref="manchester_nebula_fc_club"/>
            <field name="hr_department_id" ref="dept_executive_board_lucerne"/>
            <field name="sequence">30</field>
            <field name="active" eval="True"/>
        </record>

    </data>
</odoo>
