<odoo>
    <data noupdate="1">
        <!-- Active Member Manchester -->
        <record id="product_active_member_manchester" model="product.product">
            <field name="name">Active Member Manchester</field>
            <field name="type">service</field>
            <field name="list_price">200</field>
            <field name="standard_price">200</field>
            <field name="uom_id" ref="uom.product_uom_unit"/>
            <field name="uom_po_id" ref="uom.product_uom_unit"/>
            <field name="default_code">ACT-MEM-MAN</field>
            <field name="company_id" ref="manchester_nebula_fc_main_club"/>
            <field name="currency_id" ref="base.GBP"/>
            <field name="taxes_id" eval="[(5, 0, 0)]"/> <!-- No taxes -->
        </record>

        <!-- Passive Member Manchester -->
        <record id="product_passive_member_manchester" model="product.product">
